{
  "name": "partialify",
  "version": "3.1.1",
  "description": "require()-able HTML, CSS, JSON and (potentially) more",
  "main": "index.js",
  "scripts": {
    "test": "browserify test/*.js | testling"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bclinkinbeard/partialify"
  },
  "keywords": [
    "browser",
    "browserify",
    "browserify-transform",
    "require",
    "static",
    "asset",
    "bundle",
    "partial"
  ],
  "author": "Ben Clinkinbeard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bclinkinbeard/partialify/issues"
  },
  "dependencies": {
    "string-to-js": "0.0.1",
    "through": "~2.3.4"
  },
  "devDependencies": {
    "tape": "~2.3.2",
    "browserify": "~3.24.1"
  }
}
